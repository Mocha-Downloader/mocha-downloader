{
	"name": "mocha-downloader",
	"productName": "mocha-downloader",
	"description": "A GUI tool for searching, parsing, and downloading contents from the web.",
	"bugs": {
		"url": "https://github.com/Mocha-Downloader/mocha-downloader/issues"
	},
	"homepage": "https://github.com/Mocha-Downloader/mocha-downloader",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Mocha-Downloader/mocha-downloader.git"
	},
	"author": {
		"name": "developomp",
		"email": "developomp@gmail.com",
		"url": "https://developomp.com"
	},
	"scripts": {
		"postinstall": "ts-node .erb/scripts/fix-double-semicolon.ts && ts-node .erb/scripts/check-native-dep.ts && electron-builder install-app-deps && cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
		"icons": "ts-node ./.erb/scripts/generate-icons.ts",
		"start": "ts-node ./.erb/scripts/check-port-in-use.ts && yarn start:renderer",
		"start:main": "cross-env NODE_ENV=development electron -r ts-node/register/transpile-only ./src/main/main.ts",
		"start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
		"build": "concurrently 'yarn build:main' 'yarn build:renderer'",
		"build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
		"build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
		"rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir src",
		"package": "ts-node ./.erb/scripts/clean.ts dist && yarn build && electron-builder build --publish never",
		"lint": "cross-env NODE_ENV=development eslint . --cache --ext .js,.jsx,.ts,.tsx"
	},
	"dependencies": {
		"axios": "^0.24.0",
		"cheerio": "^1.0.0-rc.10",
		"history": "^5.1.0",
		"i18next": "^21.5.3",
		"image-size": "^1.0.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-dropzone": "^11.4.2",
		"react-i18next": "^11.14.3",
		"react-router-dom": "^6.0.2",
		"react-virtualized-auto-sizer": "^1.0.6",
		"react-window": "^1.8.6",
		"semantic-ui-css": "^2.4.1",
		"semantic-ui-react": "^2.0.4",
		"styled-components": "^5.3.3",
		"utility-types": "^3.10.0",
		"webtorrent": "^1.5.8",
		"ytdl-core": "^4.9.1",
		"ytpl": "^2.2.3"
	},
	"devDependencies": {
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.2",
		"@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
		"@types/detect-port": "^1.3.1",
		"@types/enzyme": "^3.10.10",
		"@types/enzyme-adapter-react-16": "^1.0.6",
		"@types/history": "^4.7.9",
		"@types/jest": "^27.0.3",
		"@types/node": "^16.11.10",
		"@types/react": "^17.0.37",
		"@types/react-dom": "^17.0.11",
		"@types/react-router": "^5.1.17",
		"@types/react-router-dom": "^5.3.2",
		"@types/react-virtualized-auto-sizer": "^1.0.1",
		"@types/react-window": "^1.8.5",
		"@types/rimraf": "^3.0.2",
		"@types/sharp": "^0.29.4",
		"@types/styled-components": "^5.1.15",
		"@types/terser-webpack-plugin": "^5.2.0",
		"@types/to-ico": "^1.1.1",
		"@types/webpack-env": "^1.16.3",
		"@types/webtorrent": "^0.109.2",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"browserslist-config-erb": "^0.0.3",
		"chalk": "^4.1.2",
		"concurrently": "^6.4.0",
		"core-js": "^3.19.1",
		"cross-env": "^7.0.3",
		"css-loader": "^6.5.1",
		"css-minimizer-webpack-plugin": "^3.2.0",
		"detect-port": "^1.3.0",
		"electron": "^16.0.2",
		"electron-builder": "^22.14.5",
		"electron-debug": "^3.2.0",
		"electron-devtools-installer": "^3.2.0",
		"electron-log": "^4.4.1",
		"electron-notarize": "^1.1.1",
		"electron-rebuild": "^3.2.5",
		"electron-updater": "^4.6.1",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.6",
		"enzyme-to-json": "^3.6.2",
		"eslint": "^8.3.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-erb": "^4.0.0-alpha.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-compat": "^4.0.0",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-promise": "^5.1.1",
		"eslint-plugin-react": "^7.27.1",
		"eslint-plugin-react-hooks": "^4.3.0",
		"html-webpack-plugin": "^5.5.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^27.4.3",
		"mini-css-extract-plugin": "^2.4.5",
		"prettier": "^2.5.0",
		"react-refresh": "^0.11.0",
		"react-refresh-typescript": "^2.0.2",
		"regenerator-runtime": "^0.13.9",
		"rimraf": "^3.0.2",
		"sass": "^1.43.5",
		"sass-loader": "^12.3.0",
		"sharp": "^0.29.3",
		"style-loader": "^3.3.1",
		"terser-webpack-plugin": "^5.2.5",
		"to-ico": "^1.1.5",
		"ts-jest": "^27.1.0",
		"ts-loader": "^9.2.6",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.2",
		"webpack": "^5.64.4",
		"webpack-bundle-analyzer": "^4.5.0",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.6.0",
		"webpack-merge": "^5.8.0"
	},
	"build": {
		"productName": "mocha-downloader",
		"appId": "com.developomp.mocha-downloader",
		"asar": true,
		"asarUnpack": "**\\*.{node,dll}",
		"files": [
			"dist",
			"node_modules",
			"package.json"
		],
		"afterSign": ".erb/scripts/notarize.js",
		"mac": {
			"target": {
				"target": "default",
				"arch": [
					"arm64",
					"x64"
				]
			},
			"type": "distribution",
			"hardenedRuntime": true,
			"entitlements": "assets/entitlements.mac.plist",
			"entitlementsInherit": "assets/entitlements.mac.plist",
			"gatekeeperAssess": false
		},
		"dmg": {
			"contents": [
				{
					"x": 130,
					"y": 220
				},
				{
					"x": 410,
					"y": 220,
					"type": "link",
					"path": "/Applications"
				}
			]
		},
		"win": {
			"target": [
				"portable"
			]
		},
		"linux": {
			"target": [
				"AppImage"
			],
			"category": "Utility"
		},
		"directories": {
			"app": "release/app",
			"buildResources": "assets",
			"output": "release/build"
		},
		"extraResources": [
			"./assets/**"
		],
		"publish": {
			"provider": "github",
			"owner": "Mocha-Downloader",
			"repo": "mocha-downloader"
		}
	},
	"devEngines": {
		"node": ">=14.x",
		"npm": ">=7.x"
	},
	"browserslist": []
}
