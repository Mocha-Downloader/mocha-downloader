{
	"name": "mocha-downloader",
	"productName": "mocha-downloader",
	"description": "A GUI tool for searching, parsing, and downloading contents from the web.",
	"bugs": {
		"url": "https://github.com/Mocha-Downloader/mocha-downloader/issues"
	},
	"homepage": "https://github.com/Mocha-Downloader/mocha-downloader",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Mocha-Downloader/mocha-downloader.git"
	},
	"author": {
		"name": "developomp",
		"email": "developomp@gmail.com",
		"url": "https://developomp.com"
	},
	"scripts": {
		"postinstall": "ts-node .erb/scripts/fix-double-semicolon.ts && ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps && cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
		"icons": "ts-node ./.erb/scripts/generate-icons.ts",
		"start": "ts-node ./.erb/scripts/check-port-in-use.js && yarn start:renderer",
		"start:main": "cross-env NODE_ENV=development electron -r ts-node/register/transpile-only ./src/main/main.ts",
		"start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
		"build": "concurrently 'yarn build:main' 'yarn build:renderer'",
		"build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
		"build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
		"rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir src",
		"package": "ts-node ./.erb/scripts/clean.js dist && yarn build && electron-builder build --publish never",
		"lint": "cross-env NODE_ENV=development eslint . --cache --ext .js,.jsx,.ts,.tsx"
	},
	"dependencies": {
		"@ffmpeg/core": "^0.10.0",
		"@ffmpeg/ffmpeg": "^0.10.1",
		"axios": "^0.24.0",
		"cheerio": "^1.0.0-rc.10",
		"electron-is-dev": "^2.0.0",
		"history": "^5.1.0",
		"image-size": "^1.0.0",
		"lowdb": "^3.0.0",
		"pretty-error": "^4.0.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^5.3.0",
		"semantic-ui-css": "^2.4.1",
		"semantic-ui-react": "^2.0.4",
		"styled-components": "^5.3.3"
	},
	"devDependencies": {
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
		"@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
		"@types/enzyme": "^3.10.10",
		"@types/enzyme-adapter-react-16": "^1.0.6",
		"@types/history": "^4.7.9",
		"@types/lodash": "^4.14.176",
		"@types/node": "^16.11.7",
		"@types/react": "^17.0.34",
		"@types/react-dom": "^17.0.11",
		"@types/react-router": "^5.1.17",
		"@types/react-router-dom": "^5.3.2",
		"@types/sharp": "^0.29.3",
		"@types/styled-components": "^5.1.15",
		"@types/terser-webpack-plugin": "^5.2.0",
		"@types/to-ico": "^1.1.1",
		"@types/webpack-env": "^1.16.3",
		"@typescript-eslint/eslint-plugin": "^5.3.0",
		"browserslist-config-erb": "^0.0.3",
		"chalk": "^4.1.2",
		"concurrently": "^6.3.0",
		"core-js": "^3.19.1",
		"cross-env": "^7.0.3",
		"css-loader": "^6.5.1",
		"css-minimizer-webpack-plugin": "^3.1.1",
		"detect-port": "^1.3.0",
		"electron": "^15.3.1",
		"electron-builder": "^22.13.1",
		"electron-debug": "^3.2.0",
		"electron-devtools-installer": "^3.2.0",
		"electron-log": "^4.4.1",
		"electron-rebuild": "^3.2.5",
		"electron-updater": "^4.3.9",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.6",
		"enzyme-to-json": "^3.6.2",
		"eslint": "^8.2.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-config-erb": "^4.0.3",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-compat": "^3.13.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-promise": "^5.1.1",
		"eslint-plugin-react": "^7.26.1",
		"eslint-plugin-react-hooks": "^4.2.0",
		"html-webpack-plugin": "^5.5.0",
		"identity-obj-proxy": "^3.0.0",
		"mini-css-extract-plugin": "^2.4.4",
		"prettier": "^2.4.1",
		"react-refresh": "^0.10.0",
		"react-refresh-typescript": "^2.0.2",
		"regenerator-runtime": "^0.13.9",
		"rimraf": "^3.0.2",
		"sass": "^1.43.4",
		"sass-loader": "^12.3.0",
		"sharp": "^0.29.3",
		"style-loader": "^3.3.1",
		"terser-webpack-plugin": "^5.2.4",
		"to-ico": "^1.1.5",
		"ts-loader": "^9.2.6",
		"ts-node": "^10.4.0",
		"typescript": "^4.4.4",
		"webpack": "^5.62.1",
		"webpack-bundle-analyzer": "^4.5.0",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.4.0",
		"webpack-merge": "^5.8.0"
	},
	"build": {
		"productName": "mocha-downloader",
		"appId": "com.developomp.mocha-downloader",
		"asar": true,
		"asarUnpack": "**\\*.{node,dll}",
		"files": [
			"dist",
			"node_modules",
			"package.json"
		],
		"win": {
			"target": [
				"nsis"
			]
		},
		"linux": {
			"target": [
				"AppImage"
			],
			"category": "Development"
		},
		"directories": {
			"app": "release/app",
			"buildResources": "assets",
			"output": "release/build"
		},
		"extraResources": [
			"./assets/**"
		],
		"publish": {
			"provider": "github",
			"owner": "Mocha-Downloader",
			"repo": "mocha-downloader"
		}
	},
	"devEngines": {
		"node": ">=14.x",
		"npm": ">=7.x"
	},
	"browserslist": []
}
